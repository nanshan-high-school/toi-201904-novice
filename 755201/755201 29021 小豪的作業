#include <iostream>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;
    if (a == b) {
        a -= 3;
    }
    if (a > b) {
        int c = a-b;
        cout << b << "+" << c << "=" << a;
    } else {
        int c = b-a;
        cout << a << "+" << c << "=" << b;
    }

}
